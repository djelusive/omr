{
  "name": "djelusive/omr",
  "description": "Optical Mark Recognition from PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Timothy Kent",
      "email": "timothy.kent83@gmail.com"
    }
  ],
  "keywords": [
    "omr",
    "optical mark recognition",
    "survey"
  ],
  "require": {
    "php": "8.*",
    "ext-imagick": "*",
    "symfony/console": "*"
  },
  "bin": ["bin/omr"],
  "require-dev": {
    "phpunit/phpunit": "8.*"
  },
  "autoload": {
    "psr-4": {
      "Djelusive\\OMR\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Djelusive\\OMR\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "minimum-stability": "dev"
}
